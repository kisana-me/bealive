<%= form_with(model: @image, data: { turbo: false }) do |form| %>
  <%= render "shared/error_messages", instead: @image %>

  <div class="">
    <%= form.label :name, "名前" %><br>
    <%= form.text_field :name %>
  </div>

  <div class="">
    <%= form.label :image, "画像ファイル" %><br>
    <%= form.file_field :image %>
  </div>

  <div class="">
    <%= form.submit "アップロード" %>
  </div>
<% end %>
